{
    "name": "autohome-cli",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "packageManager": "pnpm@10.6.4",
    "scripts": {
        "dev": "turbo run dev",
        "build": "turbo run build",
        "release": "turbo run release",
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint:es": "eslint",
        "lint:ts": "tsc --noEmit",
        "prepare": "husky",
        "commit": "git-cz",
        "spellcheck": "cspell lint --dot --gitignore --color --cache --show-suggestions \"(packages|apps)/**/*.@(html|js|cjs|mjs|ts|tsx|css|scss|md)\""
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-git"
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{json,md}": [
            "prettier --write"
        ]
    },
    "devDependencies": {
        "@commitlint/cli": "19.4.0",
        "@commitlint/config-conventional": "19.2.2",
        "@eslint/js": "9.9.0",
        "@types/jest": "29.5.12",
        "@types/node": "22.2.0",
        "@typescript-eslint/eslint-plugin": "8.1.0",
        "commitizen": "4.3.0",
        "cspell": "8.13.3",
        "cz-git": "1.9.4",
        "eslint": "9.9.0",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "globals": "15.9.0",
        "husky": "9.1.4",
        "lint-staged": "15.2.9",
        "madge": "8.0.0",
        "prettier": "3.3.3",
        "turbo": "2.0.12",
        "typescript": "5.5.4",
        "typescript-eslint": "8.1.0"
    }
}
